<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
        android:color="@color/parkButtonRippleColor">
    <item>
        <shape>
            <stroke
                android:width="2dp"
                android:color="@color/colorRed600"/>
            <solid android:color="@color/colorRed600"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>
    <item android:id="@android:id/mask">
        <shape>
            <stroke
                android:width="2dp"
                android:color="@color/colorRed600"/>
            <solid android:color="@android:color/white"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>

</ripple>